{
  "Vsnip sample": {
    "prefix": ["sample"],
    "body": [
      "\"$1\": {",
      "    \"prefix\": \"$1\",",
      "    \"body\": [\"$2\"],",
      "    \"description\": \"Insert $1\"",
      "}${3|\\,|}",
      "$0"
    ]
  },
  "Vsnip sample multi lines": {
    "prefix": ["sample-multi-line"],
